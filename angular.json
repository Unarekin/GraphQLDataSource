{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "GraphQLDataSource": {
      "root": "projects/graph-qldata-source",
      "sourceRoot": "projects/graph-qldata-source/src",
      "projectType": "library",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "tsConfig": "projects/graph-qldata-source/tsconfig.lib.json",
            "project": "projects/graph-qldata-source/ng-package.json"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/graph-qldata-source/src/test.ts",
            "tsConfig": "projects/graph-qldata-source/tsconfig.spec.json",
            "karmaConfig": "projects/graph-qldata-source/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/graph-qldata-source/tsconfig.lib.json",
              "projects/graph-qldata-source/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "ngx-graphqldatasource": {
      "root": "projects/ngx-graphqldatasource",
      "sourceRoot": "projects/ngx-graphqldatasource/src",
      "projectType": "library",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "tsConfig": "projects/ngx-graphqldatasource/tsconfig.lib.json",
            "project": "projects/ngx-graphqldatasource/ng-package.json"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/ngx-graphqldatasource/src/test.ts",
            "tsConfig": "projects/ngx-graphqldatasource/tsconfig.spec.json",
            "karmaConfig": "projects/ngx-graphqldatasource/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/ngx-graphqldatasource/tsconfig.lib.json",
              "projects/ngx-graphqldatasource/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    }
  },
  "defaultProject": "GraphQLDataSource"
}